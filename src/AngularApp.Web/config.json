{
  "AppSettings": {
    "SiteTitle": "AngularApp",
    "IssuerUrl": "https://idsrv3.com",
    "BaseUrl": "http://localhost:1618",
    "AuthorizationUrl": "http://localhost:1618/core",
    "ApiUrl": "http://localhost:1618/api"
  },
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Server=HOME-PC;Database=AngularApp;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  },
  "system.diagnostics": {
    "trace": {
      "-autoflush": "true",
      "-indentsize": "4",
      "listeners": {
        "add": {
          "-name": "myListener",
          "-type": "System.Diagnostics.TextWriterTraceListener",
          "-initializeData": "Trace.log"
        },
        "remove": { "-name": "Default" }
      }
    }
  }
}
